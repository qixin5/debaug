#!/bin/bash

rootdir=${DEBAUG_DIR} 

target_function_file=$1 #A file containing a list of functions (one per line) to be augmented
target_code_file=$2 #Reduced code to be augmented
line_file=$3 #line.txt generated by startandendlineprinter
origin_code_file=$4 #Origin code as reference

java -cp :${rootdir}/build/java:${rootdir}/lib/java/* edu.gatech.cc.debaug.FunctionAugmenter ${target_function_file} ${target_code_file} ${line_file} ${origin_code_file}
