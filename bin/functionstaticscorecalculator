#!/bin/bash

rootdir=${DEBAUG_DIR} 

call_graph_file=$1 #callgraph.dot generated by clang
line_file=$2 #line.txt
score_type=$3 #pr, rpr, util, rutil, etc.


java -cp :${rootdir}/build/java:${rootdir}/lib/java/*:${rootdir}/lib/java/jgrapht-1.4.0/* edu.gatech.cc.debaug.FunctionStaticScoreCalculator ${call_graph_file} ${line_file} ${score_type}

