#!/bin/bash

rootdir=${DEBAUG_DIR} 

target_codef=$1 #Code to be augmented
origin_codef=$2 #Origin code
line_file=$3 #line.txt generated by startandendlineprinter
check_type=$4 #statement or binary
check_size=$5 #Size as augmenting limit
compile_scriptf=$6 #Absolute path to compiling script (use $BIN $SRC as parameters)
getsize_scriptf=$7 #Absolute path to getsize script (use $INFILE $OUTFILE as parameters)
rslt_codef=$8
devrate=$9 #The deviation rate (of check_size) used to decide termination of augmentation. Zero means exact size should be reached to terminate.

java -cp :${rootdir}/build/java:${rootdir}/lib/java/* edu.gatech.cc.debaug.RandomAugmenter ${target_codef} ${origin_codef} ${line_file} ${check_type} ${check_size} ${compile_scriptf} ${getsize_scriptf} ${rslt_codef} ${devrate}

